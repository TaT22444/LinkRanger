# Stripe設定
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Stripe価格ID（Stripeダッシュボードで作成後に設定）
EXPO_PUBLIC_STRIPE_STANDARD_MONTHLY_PRICE_ID=price_standard_monthly_id
EXPO_PUBLIC_STRIPE_PRO_MONTHLY_PRICE_ID=price_pro_monthly_id

# Firebase Functions URL
EXPO_PUBLIC_FIREBASE_FUNCTIONS_URL=https://your-region-your-project-id.cloudfunctions.net

# Gemini API設定
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# 開発者設定
EXPO_PUBLIC_DEVELOPER_EMAILS=test@example.com,dev@example.com
EXPO_PUBLIC_ENABLE_TEST_ACCOUNTS=true

# デバッグ設定（本番リリース時は false に変更）
EXPO_PUBLIC_DEBUG_MODE=true 